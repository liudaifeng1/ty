function ay() {
	var a = document.getElementById("a");
	var b = document.getElementById("b");
	var c = document.getElementById("c");
	var kk = document.getElementById("music");
	var juo = document.getElementById("video");
	var dwe = document.getElementById("doc");
	var bv = a.innerHTML.replace(/\s/g, "");
	if(bv == "") {
		alert("content不能为空");
		document.getElementById("button").disabled = true;
	}
	if(b.value == "") {
		alert("请选择图片");
		document.getElementById("button").disabled = true;
	} else {
		if(!/\.(jpg|jpeg|png|JPG|PNG|gif)$/.test(b.value)) {
			alert("只能选择jpg/jpeg/png/gif格式的图片");
			document.getElementById("button").disaplay = true;
		} else {
			if(a.innerHTML != "") {
				getObjectURL();
				if(kk.value != "") {
					if(!/\.(mp3|ogg|mpeg|wav)$/.test(kk.value)) {
						alert("不支持的音频格式");
						musicurl = "#";
					} else {
						getObjectURL2();
						document.getElementById("fff").style.display = "";
					}
				} else {
					musicurl = "#";
				}
				if(juo.value != "") {
					if(!/\.(mp4|ogg|webm|wmv)$/.test(juo.value)) {
						alert("不支持的视频格式");
						videourl = "#";
					} else {
						getObjectURL3();
						document.getElementById("vvv").style.display = "";
					}
				} else {
					videourl = "#";
				}
				if(dwe.value != "") {
					if(!/\.(txt|rar|docx|pptx)$/.test(dwe.value)) {
						alert("不支持的文件格式");
						wenjian = "#";
					} else {
						getObjectURL4();
					}
				} else {
					wenjian = "#";
				}

				setTimeout(function() {
					var sdf = $("#c").attr("src");
					console.log(sdf);
					var tyu = document.getElementsByClassName("text")[0].innerHTML;
					var time = new Date();
					var hours = time.getHours();
					var minutes = time.getMinutes();
					var seconds = time.getSeconds();
					if(time.getHours() < 10) {
						hours = "0" + time.getHours();
					}
					if(time.getMinutes() < 10) {
						minutes = "0" + time.getMinutes();
					}
					if(time.getSeconds() < 10) {
						seconds = "0" + time.getSeconds();
					}
					var currentTime = time.getFullYear() + "年" + time.getMonth() + 1 + "月" + time.getDate() + "日" + "" + hours + ":" + minutes + ":" + seconds;
					$.ajax({
						type: "post",
						url: "new_file10.php",
						async: true,
						data: {
							sdf: sdf,
							tyu: tyu,
							currentTime: currentTime,
							music: musicurl,
							video: videourl,
							wenj: wenjian
						},
						success: function(data2) {
							if(data2 == "wrong") {
								alert("数据库访问失败");
							}
							if(data2 == "success") {
								alert("图书推荐成功!");
							}
							if(data2 == "fail") {
								alert("啊哦，总有东西会阻碍你，不怕，再试一次!");
							}
						},
						error: function() {
							alert("ajax请求失败");
						}
					});
				}, 500);
			}
		}
	}
}

