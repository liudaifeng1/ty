function back() {
	window.history.back(-1);
}

function getObjectURL() {
	//获取文件  
	var file1 = $("#imgForm").find("input")[0].files[0];

	//创建读取文件的对象  
	var reader = new FileReader();

	//创建文件读取相关的变量  
	var imgFile;

	//为文件读取成功设置事件  
	reader.onload = function(e) {
		imgFile = e.target.result;
		$("#c").attr('src', imgFile);
	};

	//正式读取文件  
	reader.readAsDataURL(file1);
}

function getObjectURL2() {
	//获取文件  
	var file1 = $("#imgForm").find("input")[1].files[0];

	//创建读取文件的对象  
	var reader = new FileReader();

	//创建文件读取相关的变量  
	var File6;

	//为文件读取成功设置事件  
	reader.onload = function(e) {
		File6 = e.target.result;
		musicurl = File6;
		document.getElementById("fff").src = musicurl;
	};

	//正式读取文件  
	reader.readAsDataURL(file1);
}

function getObjectURL3() {
	//获取文件  
	var file1 = $("#imgForm").find("input")[2].files[0];

	//创建读取文件的对象  
	var reader = new FileReader();

	//创建文件读取相关的变量  
	var File;

	//为文件读取成功设置事件  
	reader.onload = function(e) {
		File = e.target.result;
		videourl = File;
		document.getElementById("vvv").src = videourl;
		alert(videourl);
	};

	//正式读取文件  
	reader.readAsDataURL(file1);
}

function getObjectURL4() {
	//获取文件  
	var file1 = $("#imgForm").find("input")[3].files[0];

	//创建读取文件的对象  
	var reader = new FileReader();

	//创建文件读取相关的变量  
	var File1;

	//为文件读取成功设置事件  
	reader.onload = function(e) {
		File1 = e.target.result;
		wenjian = File1;
	};

	//正式读取文件  
	reader.readAsDataURL(file1);
}

