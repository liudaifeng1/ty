var vcf=0;
function clcl(){
	vcf++;
	var aaa=document.getElementsByClassName("content");
	var bbb=document.getElementsByClassName("content1");
	if(vcf%2==0){
		aaa[0].style.display="inline-block";
	bbb[0].style.display="inline-block";
	}
	else{
		aaa[0].style.display="none";
	bbb[0].style.display="none";
	}
}

function IsPC() {
	var userAgentInfo = navigator.userAgent;
	var Agents = ["Android", "iPhone",
		"SymbianOS", "Windows Phone",
		"iPad", "iPod"
	];
	var flag = true;
	for(var v = 0; v < Agents.length; v++) {
		if(userAgentInfo.indexOf(Agents[v]) > 0) {
			flag = false;
			break;
		}
	}
	return flag;
}
$(document).ready(function(){
	
})
window.onload = function() {
	var f4 = document.getElementById("fg4"); //视频转换
	var f2 = document.getElementById("fg2"); //视频录制
	var f1 = document.getElementById("fg1"); //视频下载
	var f3 = document.getElementById("fg3"); //音频录制
	var f5 = document.getElementById("fg5"); //截屏

	if(typeof(Storage) !== "undefined") {
		if(sessionStorage.getItem("x1") != undefined) {
			alert("yeah");
		} else {
			window.location.href = "DZ/index.html";
		}
	} else {
		// 抱歉! 不支持 web 存储。
	}

	var rc = IsPC();
	if(rc == false) {
		alert("你目前使用的终端是手机端，有些功能不支持");
		document.getElementById("mj").style.display = "none";
		document.getElementById("body").style.backgroundImage="url(images/1200383920.jpg)";
	} else {
		alert("welcome");
	}
	document.getElementById("a").onfocus = function() {
		document.getElementById("button").disabled = false;
	}
	$(".rr").each(function(inde) {
		$(this).click(function() {
			switch(inde) {
				case 0:
					f2.style.display = "";
					f1.style.display = "none";
					f3.style.display = "none";
					f4.style.display = "none";
					f5.style.display = "none";
					break;
				case 1:
					f5.style.display = "";
					f2.style.display = "none";
					f1.style.display = "none";
					f3.style.display = "none";
					f4.style.display = "none";
					break;
				case 2:
					f1.style.display = "";
					f2.style.display = "none";
					f5.style.display = "none";
					f3.style.display = "none";
					f4.style.display = "none";
					break;
				case 3:
					f3.style.display = "";
					f2.style.display = "none";
					f1.style.display = "none";
					f5.style.display = "none";
					f4.style.display = "none";
					break;
				case 4:
					f4.style.display = "";
					f2.style.display = "none";
					f1.style.display = "none";
					f3.style.display = "none";
					f5.style.display = "none";
					break;
			}
		})
	})
}

